<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

	<class name="" table="">
		<id name="id" type="java.lang.Long">
			<column name="id">
				<comment>唯一标识符</comment>
			</column>
			<!-- 主键 - 生成规则 -->
		</id>
		<property name="" type="java.lang.String">
			<column name="" length="" not-null="false">
				<!-- 列注释 -->
				<comment></comment>
			</column>
		</property>
		<property name="" type="java.lang.String" >
			<column name="" length="" not-null="false">
				<!-- 列注释 -->
				<comment></comment>
			</column>
		</property>
		<property name="" type="java.lang.String" >
			<column name="" length="" not-null="false">
				<!-- 列注释 -->
				<comment></comment>
			</column>
		</property>
		<!-- 枚举值配置 -->
		<property name="status" type="com.hua.constant.ext.CustomStatus">
			<type name="org.hibernate.type.EnumType">
			 	<param name="enumClass">com.hua.constant.ext.CustomStatus</param>
				<!--
				     12为java.sql.Types.VARCHAR常量值，即保存枚举的字面值到数据库。如果不指定type参数，保存枚举的索引值(从0开始)到数据库
				     <param name="type">12</param>
				  -->				
			</type>
			<column name="status" not-null="false">
				<!-- 列注释 -->
				<comment>客户状态 (0-无效, 1-未激活, 2-正常)</comment>
			</column>
		</property>						
	</class>



</hibernate-mapping>
